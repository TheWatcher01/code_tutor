    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "supertest": "^6.3.4",
    "swagger-jsdoc": "^6.2.8",
    "ts-jest": "^29.2.5",
    "typedoc": "^0.25.13",
